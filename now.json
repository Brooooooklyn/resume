{
  "version": 2,
  "builds": [
    { "src": "lambda/server-bundle.js", "use": "@now/node" }
  ],
  "env": {
    "IS_PUBLIC": "true",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "IS_PUBLIC": "true",
      "NODE_ENV": "production"
    }
  },
  "alias": ["resume.lynvv.xyz"],
  "routes": [
    {
      "src": "/.*",
      "dest": "/lambda/server-bundle.js",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    }
  ]
}
