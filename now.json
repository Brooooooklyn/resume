{
  "version": 2,
  "name": "resume",
  "builds": [
    { "src": "lambda/server-bundle.js", "use": "@now/node" }
  ],
  "env": {
    "IS_PUBLIC": true,
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "IS_PUBLIC": true,
      "NODE_ENV": "production"
    }
  },
  "public": false,
  "github": {
    "enabled": true
  },
  "alias": ["resume.lynvv.xyz"],
  "routes": [
    {
      "src": "/.*",
      "dest": "/lambda/server-bundle.js",
      "headers": {
        "cache-control": "s-maxage=86400",
        "link": "</app.css>; as=style; rel=preload, </app.js>; as=script; rel=preload"
      }
    }
  ]
}
